{
  "openapi": "3.0.0",
  "info": {
    "title": "GIZINET API",
    "version": "1.0.0",
    "description": "API documentation for managing food nutrition analysis"
  },
  "servers": [
    {
      "url": "https://itase-6.vercel.app/api",
      "description": "Production server"
    }
